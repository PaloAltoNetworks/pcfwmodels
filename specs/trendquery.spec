# Model
model:
  rest_name: trendquery
  resource_name: trendqueries
  entity_name: TrendQuery
  package: logserv
  group: core/log
  description: Answer trend queries on firewall logs.

# Attributes
attributes:
  v1:
  - name: trendResult
    description: The result of the trend query.
    type: refList
    exposed: true
    subtype: trendqueryitem
    read_only: true
    autogenerated: true
    extensions:
      refMode: pointer

  - name: trendType
    description: The type of field to get trends for.
    type: enum
    exposed: true
    required: true
    allowed_choices:
    - Traffic
    - IngressBytes
    - EgressBytes
    - ThreatsDetected
    - ThreatsBlocked
    example_value: Traffic
