# Model
model:
  rest_name: logincidentdetailsquery
  resource_name: logincidentdetailsqueries
  entity_name: LogIncidentDetailsQuery
  package: logging
  group: core/log
  description: Provide details on a Log Incident, including traffic and threat log
    information.

# Attributes
attributes:
  v1:
  - name: attackerIPs
    description: IP addresses for the attacker.
    type: list
    exposed: true
    subtype: string
    required: true
    example_value:
    - 212.44.7.91
    - 10.10.1.1

  - name: firewallName
    description: The NGFW name.
    type: string
    exposed: true
    required: true
    example_value: firewall1

  - name: logResult
    description: The result of the query.
    type: refList
    exposed: true
    subtype: firewalllog
    read_only: true
    autogenerated: true
    extensions:
      refMode: pointer

  - name: victimIPs
    description: IP addresses for the victim.
    type: list
    exposed: true
    subtype: string
    required: true
    example_value:
    - 212.44.7.92
    - 10.10.1.2
