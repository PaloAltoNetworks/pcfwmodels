# Model
model:
  rest_name: firewalloptionregion
  resource_name: firewalloptionregions
  entity_name: FirewallOptionRegion
  package: discovery
  group: core/discovery
  description: |-
    Includes information about region, along with its VPCs, for use in firewall
    configuration.
  detached: true

# Attributes
attributes:
  v1:
  - name: VPCAvailabilityZones
    description: List of VPCs and their associated availability zones.
    type: refList
    exposed: true
    subtype: firewalloptionvpc
    read_only: true
    autogenerated: true
    extensions:
      refMode: pointer

  - name: region
    description: The AWS region of the VPCs.
    type: string
    exposed: true
    read_only: true
    autogenerated: true
    example_value: us-east-1
