# Model
model:
  rest_name: pcfwaccount
  resource_name: pcfwaccounts
  entity_name: PCFWAccount
  package: ngfw
  group: onboarding
  description: Represents Prisma Cloud Firewall Account.
  get:
    description: Retrieve the status of the account.
  delete:
    description: offboard the account.
  extends:
  - '@namespaced'
  - '@identifiable-stored'
  - '@zoned'

# Attributes
attributes:
  v1:
  - name: status
    description: status of account.
    type: enum
    exposed: true
    read_only: true
    stored: true
    allowed_choices:
    - Success
    - Failed
    autogenerated: true
