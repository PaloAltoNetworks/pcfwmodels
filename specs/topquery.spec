# Model
model:
  rest_name: topquery
  resource_name: topqueries
  entity_name: TopQuery
  package: logserv
  group: core/log
  description: Answer general queries on firewall logs.

# Attributes
attributes:
  v1:
  - name: topResult
    description: The result of the top query. The top items in the category.
    type: refList
    exposed: true
    subtype: topqueryitem
    read_only: true
    autogenerated: true
    extensions:
      refMode: pointer

  - name: topType
    description: The type of field to get top results for.
    type: enum
    exposed: true
    required: true
    allowed_choices:
    - SourceIP
    - DestinationIP
    - SourceCountry
    - DestinationCountry
    - Application
    - Attacker
    - AttackVictim
    - Attack
    - AttackType
    - AttackedApplication
    - URLCategory
    example_value: SourceIP
