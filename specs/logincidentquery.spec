# Model
model:
  rest_name: logincidentquery
  resource_name: logincidentqueries
  entity_name: LogIncidentQuery
  package: logging
  group: core/log
  description: Represents a query for security incidents.

# Attributes
attributes:
  v1:
  - name: firewallName
    description: The NGFW name.
    type: string
    exposed: true

  - name: logResult
    description: The result of the query.
    type: refList
    exposed: true
    subtype: logincident
    read_only: true
    autogenerated: true
    extensions:
      refMode: pointer
